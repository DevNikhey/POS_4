<Window x:Class="Philosophen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Philosophen"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="name1" HorizontalAlignment="Left" Margin="24,35,0,0" TextWrapping="Wrap" Text="Name 1" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.507,0.481"/>
        <TextBox x:Name="name2" HorizontalAlignment="Left" Margin="24,58,0,0" TextWrapping="Wrap" Text="Name 2" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.507,0.481"/>
        <TextBox x:Name="name3" HorizontalAlignment="Left" Margin="24,81,0,0" TextWrapping="Wrap" Text="Name 3" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.507,0.481"/>
        <TextBox x:Name="name4" HorizontalAlignment="Left" Margin="24,104,0,0" TextWrapping="Wrap" Text="Name 4" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.507,0.481"/>
        <TextBox x:Name="name5" HorizontalAlignment="Left" Margin="24,127,0,0" TextWrapping="Wrap" Text="Name 5" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.507,0.481"/>

        <TextBox x:Name="ddenken" HorizontalAlignment="Left" Margin="181,49,0,0" TextWrapping="Wrap" Text="Durchs. Denken" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="vdenken" HorizontalAlignment="Left" Margin="181,72,0,0" TextWrapping="Wrap" Text="Varianz Denken" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="dessen" HorizontalAlignment="Left" Margin="181,95,0,0" TextWrapping="Wrap" Text="Durchs. Essen" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="vessen" HorizontalAlignment="Left" Margin="181,118,0,0" TextWrapping="Wrap" Text="Varianz Essen" VerticalAlignment="Top" Width="120"/>

        <Button x:Name="start" Click="start_Click" Content="Start" HorizontalAlignment="Left" Margin="64,176,0,0" VerticalAlignment="Top"/>
        <Button x:Name="stop" Content="Stop" Click="stop_Click" HorizontalAlignment="Left" Margin="97,176,0,0" VerticalAlignment="Top"/>

        <Image Margin="356,49,107,78" Source="/Image1.png" Stretch="Fill"/>
        <TextBox x:Name="phil1" Text="Label" HorizontalAlignment="Left" Margin="508,100,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.511,0.579"/>
        <TextBox x:Name="phil2" Text="Label" HorizontalAlignment="Left" Margin="650,203,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="phil3" Text="Label" HorizontalAlignment="Left" Margin="596,356,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="phil4" Text="Label" HorizontalAlignment="Left" Margin="412,356,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.242,1.103"/>
        <TextBox x:Name="phil5" Text="Label" HorizontalAlignment="Left" Margin="366,203,0,0" VerticalAlignment="Top"/>

        <TextBox x:Name="eatCounter" Text="Counter: 0" HorizontalAlignment="Left" Margin="24,217,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
